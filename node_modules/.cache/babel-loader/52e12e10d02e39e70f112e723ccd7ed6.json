CAAA,EAAA,QAAA;AACA,UAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,GAAA,CAAA,EAAA,OAAA;AACA;AACA,OAPA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OATA;AAUA,KAZA;AAaA,IAAA,UAbA,sBAaA,KAbA,EAaA;AACA,UAAA,KAAA,KAAA,MAAA,EAAA;AACA,aAAA,IAAA,GAAA,KAAA,IAAA,GAAA,CAAA;AACA,OAFA,MAEA,IAAA,KAAA,KAAA,MAAA,IAAA,KAAA,IAAA,GAAA,CAAA,EAAA;AACA,aAAA,IAAA,GAAA,KAAA,IAAA,GAAA,CAAA;AACA;AACA;AAnBA;AA1BA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"row\">\n      <div class=\"column\">\n        {{ title }} \n      </div>\n      <div class=\"column\">\n        <a class=\"round\" @click=\"changePage('prev')\"> &#8249;</a> \n        {{ page }}\n        <a class=\"round\" @click=\"changePage('next')\"> &#8250; </a>\n      </div>  \n    </div>\n    <div>\n      <Question\n        v-model=\"page\"\n        :questionDesc=\"questionText\" \n        :question=\"quest\" \n        :answer=\"answerList\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport Question from './components/Question.vue'\nimport axios from 'axios'\nimport './assets/scss/app.scss'\n\nexport default {\n  name: 'App',\n  components: {\n    Question\n  },\n  props: {\n    title: {\n      default: \"TPS - Penalaran Umum\",\n    }\n  },\n  data () {\n    return {\n      page: 1,\n      questionText : \"\",\n      quest: \"\",\n      answerList: [],\n    }\n  },\n  watch: {\n    page () {\n      this.onLoad()\n    }\n  },\n  mounted () {\n    this.onLoad()\n  },\n  methods: {\n    onLoad () {\n      axios.get('http://localhost:3000/questions').then(res => {\n        if(res.status == 200) {\n          console.log(res.data.length)\n          this.questionText = res.data[this.page - 1].questionDescription\n          this.quest = res.data[this.page - 1].question\n          this.answerList = res.data[this.page - 1].answers\n        }\n      }).catch ((err) => {\n        console.log(err);\n      })\n    },\n    changePage (event) {\n      if (event === \"next\" ) {\n        this.page = this.page + 1\n      } else if (event === \"prev\" && this.page > 1) {\n        this.page = this.page - 1\n      }\n    }\n  }\n}\n</script>\n\n"],"sourceRoot":"src"}]}