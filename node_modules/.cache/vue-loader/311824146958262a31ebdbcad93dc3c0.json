nd */\n.container input:checked ~ .checkmark {\n  background-color: #2196F3;\n}\n.question-box {\n  background-color: rgb(255, 255, 255);\n  border-radius: 10px;\n  width: 600px;\n  border: 1px solid lightgray;\n  padding: 20px;\n  margin: 20px;\n  text-align: left;\n}\n\n.question-answer {\n  margin: 20px;\n  width: 650px;\n  text-align: left;\n}\n.question {\n  font-weight: bold;\n}\n</style>\n"]}]}